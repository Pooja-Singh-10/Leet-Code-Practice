class Solution {
      public int searchInsert(int[] nums, int target) {
        
          int n=nums.length;
          
        for (int i=0; i<n;i++)
        {
           
            if(nums[i]==target)
             return i;
                
            if(nums[i]>target)
                 return  i;
               
        }
                    return n;  // if target is greater than all the elements of array, then it would be placed in the end (ie. n)  
    
    }
  
}


